function add(x,y) 
{
    return x + y
}

class Point {
    function __init__(x,y) {
        this.x = x
        this.y = y
    }

    function display() {
        concat("Point(",this.x,",",this.y,")")
    }

    function _add_(other) {
        var result = new Point(0,0)
        result.x = this.x + other.x
        result.y = this.y + other.y

        return result

    }
}